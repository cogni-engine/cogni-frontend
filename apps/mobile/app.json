{
  "expo": {
    "name": "Cogno",
    "slug": "cogni-mobile",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/cogno-icon.png",
    "scheme": "cogni",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "buildNumber": "7",
      "bundleIdentifier": "com.cogno.ai.mobile",
      "usesAppleSignIn": true,
      "backgroundColor": "#000000",
      "infoPlist": {
        "CFBundleAllowMixedLocalizations": true,
        "NSCameraUsageDescription": "We use the camera to take photos you can share in group chats or with Cogni AI.",
        "NSPhotoLibraryUsageDescription": "We access your photo library so you can choose images to send in group chats or to Cogni AI.",
        "NSPhotoLibraryAddUsageDescription": "We save images from group chats or Cogni to your photo library when you choose to save them.",
        "NSMicrophoneUsageDescription": "We use the microphone for voice messages and voice input features.",
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true,
          "NSAllowsLocalNetworking": true
        },
        "ITSAppUsesNonExemptEncryption": false,
        "UIKeyboardAppearance": "Dark"
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#000000",
        "foregroundImage": "./assets/images/cogno-icon.png",
        "backgroundImage": "./assets/images/cogno-icon.png",
        "monochromeImage": "./assets/images/cogno-icon.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "POST_NOTIFICATIONS"
      ],
      "package": "com.cogno.ai.mobile",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "locales": {
      "ja": "./locales/ja.json"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-apple-authentication",
      "expo-image-picker",
      [
        "expo-localization",
        {
          "supportedLocales": {
            "ios": ["en", "ja"],
            "android": ["en", "ja"]
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "eca7ce65-e179-43c8-8e0a-74460673cc32"
      }
    },
    "owner": "cogno.ai"
  }
}
