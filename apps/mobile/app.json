{
  "expo": {
    "name": "Cogni",
    "slug": "cogni-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/cogno-icon.png",
    "scheme": "cogni",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "buildNumber": "2",
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true,
          "NSAllowsLocalNetworking": true
        },
        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.cogno.ai.mobile"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#000000",
        "foregroundImage": "./assets/images/cogno-icon.png",
        "backgroundImage": "./assets/images/cogno-icon.png",
        "monochromeImage": "./assets/images/cogno-icon.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "POST_NOTIFICATIONS"
      ],
      "blockedPermissions": [
        "android.permission.CAMERA"
      ],
      "package": "com.cogno.ai.mobile",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-apple-authentication",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/cogno-icon.png",
          "color": "#ffffff",
          "sounds": [],
          "mode": "production",
          "enableBackgroundRemoteNotifications": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "eca7ce65-e179-43c8-8e0a-74460673cc32"
      }
    },
    "owner": "cogno.ai"
  }
}
