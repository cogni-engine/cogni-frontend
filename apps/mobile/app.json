{
  "expo": {
    "name": "Cogno",
    "slug": "cogni-mobile",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/cogno-icon.png",
    "scheme": "cogni",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "buildNumber": "6",
      "bundleIdentifier": "com.cogno.ai.mobile",
      "usesAppleSignIn": true,
      "backgroundColor": "#000000",
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true,
          "NSAllowsLocalNetworking": true
        },
        "ITSAppUsesNonExemptEncryption": false,
        "UIKeyboardAppearance": "Dark",
        "NSCameraUsageDescription": "Cogno uses the camera to take photos that you can share in group chats or send to the Cogno AI.",
        "NSPhotoLibraryUsageDescription": "Cogno uses your photo library to let you select images to share in group chats or send to the Cogno AI.",
        "NSPhotoLibraryAddUsageDescription": "Cogno saves images from group chats or the Cogno AI to your photo library."

      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#000000",
        "foregroundImage": "./assets/images/cogno-icon.png",
        "backgroundImage": "./assets/images/cogno-icon.png",
        "monochromeImage": "./assets/images/cogno-icon.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "POST_NOTIFICATIONS"
      ],
      "package": "com.cogno.ai.mobile",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-apple-authentication"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "eca7ce65-e179-43c8-8e0a-74460673cc32"
      }
    },
    "owner": "cogno.ai"
  }
}
