{
  "expo": {
    "name": "Cogno",
    "slug": "cogni-mobile",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/cogno-icon.png",
    "scheme": "cogni",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "buildNumber": "6",
      "bundleIdentifier": "com.cogno.ai.mobile",
      "usesAppleSignIn": true,
      "backgroundColor": "#000000",
      "infoPlist": {
        "CFBundleDevelopmentRegion": "en",
        "CFBundleLocalizations": [
          "en",
          "ja"
        ],
        "NSCameraUsageDescription": {
          "en": "We use the camera to take photos you can share in group chats or with Cogni AI.",
          "ja": "写真を撮影して、グループチャットやCogni AIに共有するためにカメラを使用します。"
        },
        "NSPhotoLibraryUsageDescription": {
          "en": "We access your photo library so you can choose images to send in group chats or to Cogni AI.",
          "ja": "グループチャットやCogni AIに送信する画像を選択するために写真ライブラリへアクセスします。"
        },
        "NSPhotoLibraryAddUsageDescription": {
          "en": "We save images from group chats or Cogni to your photo library when you choose to save them.",
          "ja": "グループチャットやCogniから受信した画像を保存するために写真ライブラリへの追加アクセスを使用します。"
        },
        "NSMicrophoneUsageDescription": {
          "en": "We use the microphone for voice messages and voice input features.",
          "ja": "音声メッセージの送信や音声入力機能のためにマイクを使用します。"
        },
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true,
          "NSAllowsLocalNetworking": true
        },
        "ITSAppUsesNonExemptEncryption": false,
        "UIKeyboardAppearance": "Dark"
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#000000",
        "foregroundImage": "./assets/images/cogno-icon.png",
        "backgroundImage": "./assets/images/cogno-icon.png",
        "monochromeImage": "./assets/images/cogno-icon.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "POST_NOTIFICATIONS"
      ],
      "package": "com.cogno.ai.mobile",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-apple-authentication",
      "expo-image-picker",
      [
        "expo-localization",
        {
          "supportedLocales": {
            "ios": ["en", "ja"],
            "android": ["en", "ja"]
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "eca7ce65-e179-43c8-8e0a-74460673cc32"
      }
    },
    "owner": "cogno.ai"
  }
}
